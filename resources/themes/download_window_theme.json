{
    "#progress_bar": 
        {
            "colours" : 
            {
                "normal_text": "#FFFFFF",
                "filled_bar": "#5900DF,#5100CA,180",
                "unfilled_bar": "#575757"
            },

            "misc":
            {
                "shape": "rounded_rectangle",
                "shape_corner_radius": "20",
                "padding": "6,4"
            }
                
        },
        
    "#download_label":
        {
            
            "colours":
                {
                    "normal_text": "#6600FF"
                },
            "font":
                {
                    "name": "montserrat",
                    "size": "50",
                    "bold": "1",
                    "italic": "0"
                }
                
        },

    "#warning":
        {
            "colours":
            {
               "dark_bg":"#300077",
               "normal_border": "#26005E"
            },
      
            "misc":
            {
               "shape": "rounded_rectangle",
               "shape_corner_radius": "20",
               "border_width": "3",
               "shadow_width": "15",
               "enable_title_bar": "0"
            }
        },
    "#warning.#dismiss_button":
        {
            "colours":
            {   
                "normal_bg": "#5000C9",
                "hovered_bg": "#6500FD",
                "hovered_text": "#FFFFFF",
                "disabled_bg":"#4100A2"
            },
            "misc":
            {
                "shape": "rounded_rectangle",
                "shape_corner_radius": "25,25,25,25",
                "border_width": "2",
                "shadow_width": "2"
            }
        },
    "#warning.text_box":
        {
            "colours":
                {
                "dark_bg":"#300077",
                "normal_border": "#300077"
                }
        },
    
    "#confirmation_window":
